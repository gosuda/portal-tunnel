relays:
  - name: thumbgo
    urls:
      - wss://portal.thumbgo.kr/relay
  - name: gosuda
    urls:
      - wss://portal.gosuda.org/relay

services:
  - name: test-thumbgo
    relayPreference:
      - thumbgo
      - gosuda
    target: localhost:1013
    protocols:
      - http/1.1
      - h2
    metadata:
      description: "Service description"
      tags:
      - tag1
      - tag2
      thumbnail: "https://example.com/image.png"
      owner: "owner-name"
      hide: false
  - name: test-thumbgo2
    relayPreference:
      - thumbgo
      - gosuda
    target: localhost:1014
    protocols:
      - http/1.1
      - h2        

